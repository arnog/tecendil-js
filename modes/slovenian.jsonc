        "nj$": "{nuumen}[double-dot-below]",
    "ľ": "{lambe}[double-dot-below]",
        "lj$": "{lambe}[double-dot-below]",

//Doubled consonants
    "tt": "{tinco}[bar-below]",
    "pp": "{parma}[bar-below]",
    "čč": "{calma}[bar-below]",
    "kk": "{quesse}[bar-below]",
    "dd": "{ando}[bar-below]",
    "bb": "{umbar}[bar-below]",
    "dždž": "{anga}[bar-below]",
    "gg": "{ungwe}[bar-below]",
    "ff": "{formen}[bar-below]",
    "šš": "{harma}[bar-below]",
    "vv": "{ampa}[bar-below]",
    "žž": "{anca}[bar-below]",
    "nn": "{nuumen}[bar-below]",
    "mm": "{malta}[bar-below]",
    "ŋŋ": "{nwalme}[bar-below]",
    "rr": "{roomen}[bar-below]",
    "ll": "{lambe}[bar-below]",
    "ss": "{silme}[bar-below]",
    "zz": "{esse}[bar-below]",
    "hh": "{hyarmen}[bar-below]",
    "jj": "{yanta}[bar-below]",
    "cc": "{tinco}[bar-below][right-hook-below]",
    "dzdz": "{ando}[bar-below][right-hook-below]",
    "ññ": "{nuumen}[bar-below][double-dot-below]",
    "ľľ": "{lambe}[bar-below][double-dot-below]",
//VOWELS
    "ê": "[double-acute]",
    "ô": "[double-right-curl]",
    "è": "[double-acute]",
    "ò": "[double-right-curl]",
    "ë": "[dot-below]",//ë marks ə
//Diphthongs
    "au": "[triple-dot-above]{vala}",
    "eu": "[acute]{vala}",
        "êu": "[double-acute]{vala}",
        "èu": "[double-acute]{vala}",
    "iu": "[dot-above]{vala}",
    "ou": "[right-curl]{vala}",
        "ôu": "[double-right-curl]{vala}",
        "òu": "[double-right-curl]{vala}",
    "uu": "[left-curl]{vala}",
    "ëu": "[dot-below]{vala}",

    "ai": "[triple-dot-above]{anna}",
    "ei": "[acute]{anna}",
        "êi": "[double-acute]{anna}",
        "èi": "[double-acute]{anna}",
    "oi": "[right-curl]{anna}",
        "ôi": "[double-right-curl]{anna}",
        "òi": "[double-right-curl]{anna}",
    "ui": "[left-curl]{anna}",
//Doubled Vowels
    "ee": "[acute]{telco}[acute]",
    "ii": "[dot-above]{telco}[dot-above]",
    "oo": "[right-curl]{telco}[right-curl]",
    "uu": "[left-curl]{telco}[left-curl]",

    "ëë": "[dot-below]{telco}[dot-below]"
  }
