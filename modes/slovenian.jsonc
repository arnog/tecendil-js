        "ś": "{silme-nuquerna}",
    "z": "{esse}",
        "ź": "{esse-nuquerna}",
    "h": "{hyarmen}",
    "j": "{yanta}",

    "c": "{tinco}[right-hook-below]",
    "dz": "{ando}[right-hook-below]",

    "Š": "{HARMA}",
