  "words": {
    "trl": "{tinco}[dot-below]{roomen}{vala}",
    "krt": "{quesse}[dot-below]{tinco}",
    "vrv": "{ampa}[dot-below]{roomen}{vala}",
    "pesek": "{parma}[acute]s[dot-below]{quesse}",
    "pes": "{parma}[dot-below]{silme}",
    "megla": "{malta}[dot-below]{ungwe}{lambe}[triple-dot-above]",
    "naiven": "{nuumen}[triple-dot-above]{telco}[dot-above]{ampa}[dot-below]{nuumen}",
    "kofein": "{quesse}[double-right-curl]{formen}[double-acute]{telco}[dot-above]{nuumen}",
    "šel": "{harma}[dot-below]{vala}"
  },
  "map": {
//CONSONANTS
    "t": "{tinco}",
    "p": "{parma}",
    "č": "{calma}",
    "k": "{quesse}",
    "d": "{ando}",
    "b": "{umbar}",
    "dž": "{anga}",
    "g": "{ungwe}",
    "f": "{formen}",
    "š": "{harma}",
    "v": "{ampa}",
    "ž": "{anca}",
